{
	"name": "tommcfarlin/remove-empty-shortcodes",
	"description": "Removes shortcodes from standard posts.",
	"type": "wordpress-plugin",
	"license": "GPL-3.0-or-later",
	"homepage": "https://github.com/tommcfarlin/remove-empty-shortcodes/issues",
	"authors": [
		{
			"name": "Tom McFarlin",
			"email": "tom@tommcfarlin.com",
			"homepage": "https://tommcfarlin.com"
		}
	],
	"support": {
		"issues": "https://github.com/tommcfarlin/remove-empty-shortcodes/issues"
	},
	"config": {
		"preferred-install": "dist",
		"platform": {
			"php": "7.2"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"php": "7.2",
		"composer/installers": "^1.4"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.5",
		"friendsofphp/php-cs-fixer": "^2.16",
		"phpro/grumphp": "^0.12.0",
		"sensiolabs/security-checker": "^4.1",
		"nikic/php-parser": "^3.1",
		"phpmd/phpmd": "^2.7"
	},
	"autoload": {
		"psr-4": {
			"RESC\\": "src/"
		}
	},
	"minimum-stability": "stable"
}

